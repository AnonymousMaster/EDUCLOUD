
 <div class="fields">


  <table id="listing" align="center" width="100%" cellpadding="1" cellspacing="1">


    <%= image_tag("loader.gif",:align => "absmiddle",:border => 0,:id => "loader", :style =>"display: none;" ) %>

    <tr>

      <td class="text-input" id="person_store_item_id" >
        <%=  f.text_field :user_id, :user_id=> @current_user.id, :value=>@current_user.first_name %>
      </td>
      <td class="text-input-bg1">  <%= f.select :store_item_id, StoreItem.all.map{|i|[i.item_name, i.id]}, {:prompt=>"Select an Item"}, {:id =>"person_store_item_id"} %></td>
      <td class="text-input"><%= f.text_field :quantity%></td>
      <td class="text-input" ><%=  f.text_field :discount %></td>
       <td class="text-input" ><%=  f.text_field :tax %></td>
      <td id="remove"> <%= link_to_remove_fields image_tag("/images/buttons/Delete.png",:border => 0) , f %>
    </td>
    </tr>
  </table>


    </div>

